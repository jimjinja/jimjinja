{
  "name": "jimjinja",
  "version": "0.0.6",
  "description": "IoT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage --no-cache",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "lint:fixdry": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix-dry-run src test",
    "pack": "npm run pack:main",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config ./config/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "compress": "cd dist; for file in `ls`; do zip -r -y $file.zip $file; done"
  },
  "repository": "https://github.com/jimjinja/jimjinja",
  "keywords": [
    "electron",
    "iot",
    "jimjinja"
  ],
  "author": "Jingun Jung",
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.3.0",
    "chromedriver": "^2.35.0",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "cross-spawn": "^6.0.3",
    "css-loader": "^0.28.9",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.8.1",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.3",
    "electron-packager": "^10.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.1.4",
    "jest-serializer-vue": "^0.3.0",
    "multispinner": "^0.2.1",
    "nightwatch": "^0.9.19",
    "node-loader": "^0.6.0",
    "selenium-server": "^3.8.1",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "vue-html-loader": "^1.2.4",
    "vue-jest": "^1.4.0",
    "vue-loader": "^13.7.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "element-ui": "^2.0.11",
    "vue": "^2.5.13"
  }
}
